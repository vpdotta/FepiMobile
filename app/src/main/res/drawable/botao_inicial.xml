<?xml version="1.0" encoding="UTF-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <stroke
        android:width="1dp"
        android:color="#a6d6fc"/>
    <corners
        android:radius="7dp" />

    <padding
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp"/>

    <solid android:color="#DAA520"/>

    <gradient
        android:angle="270"
        android:startColor="#DAA520"
        android:endColor="#ffffff"
        />

</shape>
